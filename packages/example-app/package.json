{
  "name": "@project/example-app",
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "@chakra-ui/react": "^2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^6",
    "next": "^12",
    "next-transpile-modules": "^9",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^4",
    "wadesta": "*"
  },
  "devDependencies": {
    "@types/node": "^16",
    "@types/react": "^18"
  },
  "eslintConfig": {
    "extends": [
